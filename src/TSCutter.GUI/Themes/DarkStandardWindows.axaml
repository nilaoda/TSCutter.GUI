<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:common="clr-namespace:Classic.CommonControls;assembly=Classic.CommonControls.Avalonia"
                    xmlns:system="clr-namespace:System;assembly=netstandard">
    <Color x:Key="{x:Static common:SystemColors.ActiveBorderColorKey}">#ff2d2d2d</Color>
    <Color x:Key="{x:Static common:SystemColors.ActiveCaptionColorKey}">#ff1c2526</Color>
    <Color x:Key="{x:Static common:SystemColors.ActiveCaptionTextColorKey}">#ffdadada</Color>
    <Color x:Key="{x:Static common:SystemColors.AppWorkspaceColorKey}">#ff1e1e1e</Color>
    <Color x:Key="{x:Static common:SystemColors.ControlColorKey}">#ff333333</Color>
    <Color x:Key="{x:Static common:SystemColors.ControlDarkColorKey}">#ff2a2a2a</Color>
    <Color x:Key="{x:Static common:SystemColors.ControlDarkDarkColorKey}">#ff1a1a1a</Color>
    <Color x:Key="{x:Static common:SystemColors.ControlLightColorKey}">#ff4a4a4a</Color>
    <Color x:Key="{x:Static common:SystemColors.ControlLightLightColorKey}">#ff6a6a6a</Color>
    <Color x:Key="{x:Static common:SystemColors.ControlTextColorKey}">#ffdadada</Color>
    <Color x:Key="{x:Static common:SystemColors.DesktopColorKey}">#ff121212</Color>
    <Color x:Key="{x:Static common:SystemColors.GradientActiveCaptionColorKey}">#ff2e3b4e</Color>
    <Color x:Key="{x:Static common:SystemColors.GradientInactiveCaptionColorKey}">#ff3a3a3a</Color>
    <Color x:Key="{x:Static common:SystemColors.GrayTextColorKey}">#ff7a7a7a</Color>
    <Color x:Key="{x:Static common:SystemColors.HighlightColorKey}">#ff2e4b7e</Color>
    <Color x:Key="{x:Static common:SystemColors.HighlightTextColorKey}">#ffe0e0e0</Color>
    <Color x:Key="{x:Static common:SystemColors.HotTrackColorKey}">#ff4a6496</Color>
    <Color x:Key="{x:Static common:SystemColors.InactiveBorderColorKey}">#ff2d2d2d</Color>
    <Color x:Key="{x:Static common:SystemColors.InactiveCaptionColorKey}">#ff252525</Color>
    <Color x:Key="{x:Static common:SystemColors.InactiveCaptionTextColorKey}">#ff8a8a8a</Color>
    <Color x:Key="{x:Static common:SystemColors.InfoColorKey}">#ff2a2a2a</Color>
    <Color x:Key="{x:Static common:SystemColors.InfoTextColorKey}">#ffdadada</Color>
    <Color x:Key="{x:Static common:SystemColors.MenuColorKey}">#ff2d2d2d</Color>
    <Color x:Key="{x:Static common:SystemColors.MenuBarColorKey}">#ff252525</Color>
    <Color x:Key="{x:Static common:SystemColors.MenuHighlightColorKey}">#ff3a5a9a</Color>
    <Color x:Key="{x:Static common:SystemColors.MenuTextColorKey}">#ffdadada</Color>
    <Color x:Key="{x:Static common:SystemColors.ScrollBarColorKey}">#ff333333</Color>
    <Color x:Key="{x:Static common:SystemColors.WindowColorKey}">#ff1e1e1e</Color>
    <Color x:Key="{x:Static common:SystemColors.WindowFrameColorKey}">#ff2d2d2d</Color>
    <Color x:Key="{x:Static common:SystemColors.WindowTextColorKey}">#ffdadada</Color>

    <system:Double x:Key="{x:Static common:SystemParameters.HorizontalScrollBarHeightKey}">16</system:Double>
    <system:Double x:Key="{x:Static common:SystemParameters.VerticalScrollBarWidthKey}">16</system:Double>
    <system:Double x:Key="{x:Static common:SystemParameters.HorizontalScrollBarButtonWidthKey}">16</system:Double>
    <system:Double x:Key="{x:Static common:SystemParameters.VerticalScrollBarButtonHeightKey}">16</system:Double>
    <system:Double x:Key="{x:Static common:SystemParameters.HorizontalScrollBarThumbWidthKey}">16</system:Double>
    <system:Double x:Key="{x:Static common:SystemParameters.MenuBarHeightKey}">18</system:Double>

    <system:Double x:Key="{x:Static common:SystemParameters.WindowCaptionHeightKey}">18</system:Double>
    <system:Double x:Key="{x:Static common:NonClientMetrics.CaptionFontSizeKey}">11</system:Double>
    <FontFamily x:Key="{x:Static common:NonClientMetrics.CaptionFontKey}">fonts:Tahoma#Tahoma, Tahoma, $Default</FontFamily>
    <FontFamily x:Key="ContentControlThemeFontFamily">fonts:Tahoma#Tahoma, Tahoma, $Default</FontFamily>
    <system:Double x:Key="FontSizeSmall">9</system:Double>
    <system:Double x:Key="FontSizeNormal">11</system:Double>
    <system:Double x:Key="FontSizeLarge">13</system:Double>
    
    <SolidColorBrush x:Key="ThemeForegroundBrush" Color="{DynamicResource {x:Static common:SystemColors.ControlTextColorKey}}" />
    <SolidColorBrush x:Key="DataGridColumnHeaderForegroundBrush" Color="{DynamicResource {x:Static common:SystemColors.ControlTextColorKey}}" />
</ResourceDictionary>